	<script>
	  $(document).ready(function() {

	    
	
		var $container = $('.masonry');
		
		$container.masonry({
	      itemSelector: '.box'
	    });
		
		$container.imagesLoaded( function(){
		  $container.masonry({
		    itemSelector : '.box'
		  });
		});
		
		$(".fancybox").fancybox({
			'transitionIn'	:	'elastic',
			'transitionOut'	:	'elastic',
			'speedIn'		:	600, 
			'speedOut'		:	200
		});

	  });
