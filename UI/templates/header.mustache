<!doctype html>
<html class="" lang="en" xmlns:fb="http://ogp.me/ns/fb#">
<head>
	
	<meta charset="utf-8"/>
	<title>MapleCrew - Not your mama's site</title>
	
	<link rel="icon" href="/UI/img/favicon.ico" type="image/x-icon"> 
	<link rel="shortcut icon" href="/UI/img/favicon.ico" type="image/x-icon">
	
	<link rel="stylesheet/less" type="text/css" href="/UI/less/bootstrap.less">
	<link rel="stylesheet" href="/UI/js/fancybox/jquery.fancybox-1.3.4.css" type="text/css" media="screen" />
	
	
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" type="text/javascript"></script>
	
	<script src="/UI/js/less.js" type="text/javascript"></script>
	
	<script src="/UI/js/bootstrap-transition.js"></script>
	<script src="/UI/js/bootstrap-alert.js"></script>
	<script src="/UI/js/bootstrap-modal.js"></script>
	<script src="/UI/js/bootstrap-dropdown.js"></script>
	<script src="/UI/js/bootstrap-scrollspy.js"></script>
	<script src="/UI/js/bootstrap-tab.js"></script>
	<script src="/UI/js/bootstrap-tooltip.js"></script>
	<script src="/UI/js/bootstrap-popover.js"></script>
	<script src="/UI/js/bootstrap-button.js"></script>
	<script src="/UI/js/bootstrap-collapse.js"></script>
	<script src="/UI/js/bootstrap-carousel.js"></script>
	<script src="/UI/js/bootstrap-typeahead.js"></script>
	
	<script src="/UI/js/jquery.masonry.min.js"></script>
	<script type="text/javascript" src="/UI/js/fancybox/jquery.fancybox-1.3.4.pack.js"></script>
	<script type="text/javascript" src="/UI/js/fancybox/jquery.easing-1.3.pack.js"></script>
	<script type="text/javascript" src="/UI/js/fancybox/jquery.mousewheel-3.0.4.pack.js"></script>
	
	<script type="text/javascript" src="/UI/js/jquery.infinitescroll.js"></script>
	<script type="text/javascript" src="/UI/js/behaviors/local.js"></script>
	
	<script>
	  $(document).ready(function() {

	    
	
		var $container = $('.masonry');
		
		$container.masonry({
	      itemSelector: '.box'
	    });
		
		$container.imagesLoaded( function(){
		  $container.masonry({
		    itemSelector : '.box'
		  });
		});
		
		$container.infinitescroll(
			{
				navSelector  : '#page-nav',
				nextSelector : '#page-nav a',
				itemSelector : '.box',
				//debug : true,
				
				binder: $('.content'),
				loading: {
					finishedMsg: 'No more pages to load.'
					//img: 'http://i.imgur.com/6RMhx.gif'
				}
			},
			
			// trigger Masonry as a callback
			function( newElements ) {
				var $newElems = $( newElements );
				console.log($newElems);
				$container.masonry( 'appended', $newElems );
			}
		);
		
		$(".fancybox").fancybox({
			'transitionIn'	:	'elastic',
			'transitionOut'	:	'elastic',
			'speedIn'		:	100, 
			'speedOut'		:	200,
			'showCloseButton' : false,
			'overlayColor' : '#000',
			'overlayOpacity' : 0.6,
			'showNavArrows' : false
		});
		
		$('#fancybox-content').live("click", function() {
			$.fancybox.next();
		});

	  });
	</script>
	
</head>
<body>

