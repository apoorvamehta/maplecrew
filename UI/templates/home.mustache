<div class="page_container">

	<section class="feed">
	
		<header>
			<h1>Your Bros</h1>
		</header>
	
	</section>

	<section class="interests">

		<header>
			<h1>Your Interests</h1>
			<a href="#">+ add interests</a>
		</header>
		<div class="content">
			<div class="masonry">
				{{#interests}}
				<div class="box">
					<div class="picframe">
						<a title="{{name}} - {{caption}}" href="{{img_big}}" class="fancybox" rel="interests"><img src="{{img}}" class="pic"></a>
					</div>
					<div class="caption">
						<a href="#">{{name}}</a> {{caption}}
					</div>
				</div>
				{{/interests}}
			

			</div>
		</div>

	</section>

</div>